!!!
%html
  %head
    = stylesheet_link_tag 'compiled/screen.css', :media => 'screen, projection'
    = stylesheet_link_tag 'compiled/print.css', :media => 'print'
    = javascript_include_tag "jquery-1.4.4.min.js", "jquery-ui-1.8.8.custom.min.js", "jquery.rails.js", "application"
    = javascript_include_tag 'Groject.ImageSwitch.yui.js'
    = stylesheet_link_tag :all
    = csrf_meta_tag
    /[if lt IE 8]
      = stylesheet_link_tag 'compiled/ie.css', :media => 'screen, projection'
  %body
    .container.table
      .span-28.wrapper
        .span-28.lowlight
          = yield

